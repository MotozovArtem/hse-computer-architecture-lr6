<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.hse.edu.view.MainView">
	<VBox spacing="5.0">
		<Label text="Выберите алгоритм:" />
		<ChoiceBox fx:id="cb_algorithm" maxWidth="1.7976931348623157E308" />
	</VBox>
	<VBox fx:id="vbox_algorithmPane" HBox.hgrow="ALWAYS">

	</VBox>
	<VBox fx:id="vbox_buttonsContainer" spacing="10.0">
		<Button fx:id="btn_next" maxWidth="1.7976931348623157E308" text="Вперёд" />
		<Button fx:id="btn_prev" maxWidth="1.7976931348623157E308" text="Назад" />
		<Button fx:id="btn_delete" maxWidth="1.7976931348623157E308" text="Удалить строку" />
	</VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
